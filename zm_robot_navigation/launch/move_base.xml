<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find zm_robot_navigation)/param/global_costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find zm_robot_navigation)/param/local_costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find zm_robot_navigation)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find zm_robot_navigation)/param/global_costmap_params.yaml" command="load" />
        <!-- <rosparam file="$(find zm_robot_navigation)/param/base_local_planner_params.yaml" command="load" /> -->
        <rosparam file="$(find zm_robot_navigation)/param/dwa_local_planner_params.yaml" command="load" />
        <param name="controller_frequency" value="10.0" />
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    </node>
</launch>