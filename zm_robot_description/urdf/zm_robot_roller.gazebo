<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  
  <xacro:macro name="roller_gazebo_v0" params="prefix">
	<gazebo reference="${prefix}">
	  <selfCollide>false</selfCollide>
    <gravity>true</gravity>
    <mu1>300</mu1>
    <mu2>300</mu2>
    <kp>1e50</kp>
    <kd>1e50</kd>
    <material>Gazebo/Black</material>
  </gazebo>
  </xacro:macro>
</robot>